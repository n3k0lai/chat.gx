{
	"manifest_version": 2,

	"name": "chatkit.GX",
	"description": "a gx wrapper for chatkit: a chatgpt frontend",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
    "permissions": [
        "https://api.openai.com/*"
    ],
    "web_accessible_resources": [
        "gpt3.js"
    ],
	"sidebar_action": {
        "default_icon": {
            "19": "images/19.png",
            "38": "images/38.png"
        },
        "default_title": "chatkit.GX",
        "default_panel": "panel.html"
    }
}